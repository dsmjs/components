{
  "name": "@dsmjs/components",
  "description": "React components for the dsmjs.com site",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "repository": "dsmjs/components",
  "bugs": "https://github.com/dsmjs/components/issues",
  "homepage": "https://github.com/dsmjs/components#readme",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:md": "remark --frail .",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive src/**/*-test.js",
    "test": "npm-run-all --print-label --parallel lint:* --sequential coverage --parallel build build-storybook",
    "coverage": "nyc run-s tests:unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook --port 6006 --ci",
    "build:js": "rollup --config",
    "build": "run-s clean build:*",
    "build-storybook": "build-storybook --quiet",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "predeploy": "run-s build-storybook",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/dsmjs/components.git\" -d storybook-static/",
    "prepack": "run-s build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/register": "^7.6.2",
    "@dsmjs/babel-preset": "^1.0.16",
    "@dsmjs/eslint-config": "1.0.23",
    "@dsmjs/eslint-config-mocha": "1.0.5",
    "@dsmjs/eslint-config-react": "1.1.19",
    "@dsmjs/remark-preset-lint": "1.0.1",
    "@percy-io/percy-storybook": "2.1.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@travi/any": "2.0.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "codecov": "3.6.1",
    "commitlint-config-dsmjs": "1.0.14",
    "cz-conventional-changelog": "3.0.2",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.0",
    "gh-pages": "2.1.1",
    "husky": "3.0.8",
    "mocha": "6.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "^14.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "remark": "^11.0.1",
    "remark-cli": "^7.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "storybook-router": "0.3.4",
    "validate-commit-msg": "2.14.0",
    "webpack": "^4.41.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "gatsby-link": "^2.2.20",
    "glamor": "2.20.40",
    "glamorous": "5.0.0",
    "prop-types": "15.7.2",
    "react-simple-icons": "^1.0.0-beta.5"
  }
}
