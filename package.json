{
  "name": "@dsmjs/components",
  "description": "React components for the dsmjs.com site",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "author": "dsmJS",
  "maintainers": [
    "Matt Travi <npm@travi.org> (https://matt.travi.org/)"
  ],
  "repository": "dsmjs/components",
  "bugs": "https://github.com/dsmjs/components/issues",
  "homepage": "https://github.com/dsmjs/components#readme",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "tests:unit": "mocha --recursive test/unit/**/*-test.js",
    "test": "run-s lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "start": "start-storybook -p 6006",
    "build:js": "rollup -c",
    "build": "run-s clean build:*",
    "build-storybook": "build-storybook",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "deploy": "gh-pages -r \"https://$GH_TOKEN@github.com/dsmjs/components.git\" -d storybook-static/",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload",
    "prepublishOnly": "run-s build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@percy-io/percy-storybook": "2.0.0",
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/react": "3.4.10",
    "@travi/any": "1.7.5",
    "@travi/eslint-config-travi": "^1.6.15",
    "babel-core": "6.26.3",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "commitlint-config-dsmjs": "1.0.3",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "gatsby": "1.9.277",
    "gatsby-link": "1.6.46",
    "gh-pages": "1.2.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "2.4.0",
    "husky": "1.0.0-rc.13",
    "markdownlint-cli": "0.13.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-test-renderer": "16.4.2",
    "rollup": "0.65.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.0.1",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "storybook-router": "0.3.4",
    "validate-commit-msg": "2.14.0"
  },
  "peerDependencies": {
    "gatsby-link": "^1.6.45",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "glamor": "2.20.40",
    "glamorous": "4.13.1",
    "prop-types": "15.6.2",
    "react-simple-icons": "^1.0.0-beta.5"
  }
}
